{
	"name": "@flood/element-api",
	"version": "1.0.10-beta.10",
	"description": "Internal API definitions for Element",
	"typings": "./dist/index.ts.d",
	"main": "./dist/index.js",
	"scripts": {
		"prebuild": "rm -rf dist",
		"build": "tsc",
		"postbuild": "sed -i '' 's/src/dist\\/src/g' dist/index.js",
		"build:webpack": "rm -rf dist && webpack",
		"build:ncc": "yarn ncc build -e @types/puppeteer -e assert -e comment-parser -e csv -e cuid -e d3-array -e diff -e faker -e knuth-shuffle -e prettier -e puppeteer -e recast -e sanitize-filename -e source-map -e fs-extra -e strftime -e typescript -e term-img -e vm2 -e winston -o dist ./index.ts",
		"prepublish": "in-publish && yarn build || not-in-publish"
	},
	"dependencies": {
		"@flood/element": "^1.0.10-beta.10"
	},
	"devDependencies": {
		"@types/node": "12.7.4",
		"@zeit/ncc": "^0.20.4",
		"in-publish": "^2.0.0",
		"ts-loader": "^6.0.4",
		"typescript": "^3.6.2",
		"webpack": "^4.39.3",
		"webpack-cli": "^3.3.8"
	},
	"author": "Ivan Vanderbyl <ivanvanderbyl@gmail.com>",
	"homepage": "https://element.flood.io",
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/flood-io/element.git"
	},
	"bugs": {
		"url": "https://github.com/flood-io/element/issues"
	}
}
